<%= form_for @image , :html => {:multipart => true} , class: "" do |f| %>
  <div class="form-group">
    <%= f.label :description %>
    <%= f.text_field :description , class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :picture %>
    <br>
    <%= f.file_field :picture , class: "" %>
  </div>
  <%= f.submit 'Save', class: "btn btn-primary" %>
<%end %>